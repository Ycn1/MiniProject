<template name="star">
   <view class='star-box'>
     <block wx:for="{{stars}}" wx:for-item="num" wx:key = "">
      <image wx:if="{{num == 1}}" class='star-img' src='/pages/avatar/article/icon/star.png'></image>
      <image wx:else class='star-img' src='/pages/avatar/article/icon/star-none.png'></image>
    </block>
     <text>{{score}}</text>
   </view>


</template>